---
import { SITE_TITLE, SITE_DESCRIPTION } from '../consts';
import EpisodeCard from "../components/EpisodeCard.astro";
const { title, explicit, pubDate, cover, desc, url, audioUrl, duration, index, episode, season, data} = Astro.props;
console.log(data);
---
<article class="podcast-epidode post">
<EpisodeCard
title={data.fulltitle} 
cover={cover}
desc={data.description}
url={url}
audioUrl={audioUrl}
pubDate={pubDate}
duration={duration}
explicit={explicit}
season={season}
episode={episode}
id={"card-" + index}/>
</article>